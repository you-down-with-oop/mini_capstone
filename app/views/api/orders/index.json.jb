@orders.map do |order|
  {
    id: order.id,
    subtotal: order.subtotal,
    tax: order.tax,
    total: order.total,
    carted_products: order.carted_products.map do |carted_product|
      {
        id: carted_product.id,
        quantity: carted_product.quantity,
        product: carted_product.product,
      }
    end,
  }
end
